{
  "manifest_version": 3,
  "name": "Fuck ZyBooks",
  "version": "1.0",
  "description": "Fucks ZyBooks, its in the name",

  "icons": {
    "16": "images/FZY16.png",
    "32": "images/FZY32.png",
    "48": "images/FZY48.png",
    "64": "images/FZY64.png",
    "128": "images/FZY128.png"
  },
  "content_scripts": [
    {
      "run_at": "document_end",
      "js": ["scripts/content.js"],
      "matches": [
        "https://learn.zybooks.com/zybook/*/chapter/*"
      ]
    }
  ]
}
